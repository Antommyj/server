    "debug": "~2.6.9",
    "express": "~4.15.5",
    "jade": "~1.11.0",
    "mongoose": "^4.13.7",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "serve-favicon": "~2.4.5",
    "stripe": "^5.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "nodemon": "^1.13.3"
  }
